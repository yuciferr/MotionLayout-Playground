<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.motionlayoutplayground">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MotionLayoutPlayground"
        tools:targetApi="31">
        <activity
            android:name=".SplashScreen"
            android:exported="false" />
        <activity
            android:name=".ImageDetail"
            android:exported="false" />
        <activity
            android:name=".SwipeProfile"
            android:exported="false" />
        <activity
            android:name=".KeyframeCycle"
            android:exported="false" />
        <activity
            android:name=".KeyframeInterpolation"
            android:exported="false" />
        <activity
            android:name=".KeyframePosition"
            android:exported="false" />
        <activity
            android:name=".ColorChange"
            android:exported="false" />
        <activity
            android:name=".SlideBox"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>